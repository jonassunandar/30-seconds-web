{
  "name": "30 seconds Blog",
  "dirName": "30blog",
  "repoUrl": "https://github.com/30-seconds/30-seconds-blog",
  "snippetPath": "blog_posts",
  "requirables": [
    "blog_data/snippets.json"
  ],
  "slug": "blog",
  "parser": "blogParser",
  "resolver": "blogResolver",
  "isBlog": true,
  "featured": 1,
  "theme": {
    "backColor": "#1f253d",
    "foreColor": "#edf0fc",
    "iconName": "blog"
  },
  "biasPenaltyMultiplier": 1,
  "images": {
    "name": "blog_images",
    "path": "blog_images"
  }
}
